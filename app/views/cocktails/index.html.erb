<div class="container">
  <div class="row">
    <div class="items col-sm-12 col-md-4 col-offset-4">
      <h1>Cocktails</h1>
      <ul>
        <% @cocktails.each do |cocktail|  %>
        <li>
          <%= link_to cocktail.name, cocktail_path(cocktail) %>
        </li>
        <% end  %>
      </ul>
    </div>
  </div>
</div>

